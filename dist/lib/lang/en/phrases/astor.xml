<epp:phrase id="Plugin/Screen/EPrint/AStorRestore:title">Request Archive Restore</epp:phrase>
<epp:phrase id="Plugin/Screen/EPrint/AStorRestore:description">Request restore of file from the A-Stor service to the local network.</epp:phrase>
<epp:phrase id="Plugin/Screen/EPrint/AStorRestore:action:send:title">Restore Request</epp:phrase>
<epp:phrase id="Plugin/Screen/EPrint/AStorRestore:action:cancel:title">Cacnel</epp:phrase>
<epp:phrase id="Plugin/Screen/EPrint/AStorRestore:intro">The item you have requested is not available locally but has been archived. Do you wish to request a restore of this item to the local cache? :<br />
  <div class="ep_block">
  <epc:pin name="eprint"></epc:pin>
  </div><br />
  The estimated time to restore this item <epc:pin name="restoretime"></epc:pin>.
</epp:phrase>
<epp:phrase id="Plugin/Screen/EPrint/AStorRestore:subject">Restore Request</epp:phrase>
<epp:phrase id="Plugin/Screen/EPrint/AStorRestore:mail">
  <div style="font-size: 11pt">Restore Request</div>
  <p>User <epc:pin name="user"/> (Email: <epc:pin name="email"/>) requests the restore of the following item:</p>
  <p><epc:pin name="citation"/></p>
  <p><epc:pin name="reason"/></p>
  <p><epc:pin name="url">View the item details</epc:pin>.</p>
</epp:phrase>
<epp:phrase id="Plugin/Screen/EPrint/AStorRestore:reason">No reason was given.</epp:phrase>
<epp:phrase id="Plugin/Screen/EPrint/AStorRestore:RequestOK">Restore request successful</epp:phrase>

<!-- ***** End of Plugin::Screen::EPrint::AStorRestore *****-->


